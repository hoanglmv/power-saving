    "        # Giảm learning rate nếu loss không giảm (giúp model hội tụ sâu hơn)",
    "        keras.callbacks.ReduceLROnPlateau(monitor=\"val_loss\", factor=0.5, patience=3, min_lr=1e-6)",
    "    ]",
    "",
    "    history = model.fit(",
    "        X_train, y_train,",
    "        batch_size=BATCH_SIZE,",
    "        epochs=EPOCHS,",
    "        validation_data=(X_val, y_val),",
    "        callbacks=callbacks",
    "    )",
    "    ",
